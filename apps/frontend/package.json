    "file-saver": "^2.0.5",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.53.0",
    "qs": "^6.12.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
