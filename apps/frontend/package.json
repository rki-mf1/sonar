    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.4.2",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.10",
    "vue-tsc": "^2.0.6"
